    "editor.fontLigatures": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.matchBrackets": "always",

    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Comments",
                "scope": ["comment", "punctuation.definition.comment"],
                "settings": { "fontStyle": "italic", "foreground": "#444444ff" }
            },
            {
                "name": "Variables",
                "scope": ["variable", "string constant.other.placeholder"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Colors",
                "scope": ["constant.other.color"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Invalid",
                "scope": ["invalid", "invalid.illegal"],
                "settings": { "foreground": "#ff5e5eff" }
            },
            {
                "name": "Keyword, Storage",
                "scope": ["keyword", "storage.type", "storage.modifier"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "Operator, Misc",
                "scope": [
                    "keyword.operator",
                    "constant.other.color",
                    "punctuation",
                    "meta.tag",
                    "punctuation.definition.tag",
                    "punctuation.separator.inheritance.php",
                    "punctuation.definition.tag.html",
                    "punctuation.definition.tag.begin.html",
                    "punctuation.definition.tag.end.html",
                    "punctuation.section.embedded",
                    "keyword.other.template",
                    "keyword.other.substitution"
                ],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "Tag",
                "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Function, Special Method",
                "scope": [
                    "entity.name.function",
                    "meta.function-call",
                    "variable.function",
                    "support.function",
                    "keyword.other.special-method"
                ],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Block Level Variables",
                "scope": ["meta.block variable.other"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Other Variable, String Link",
                "scope": ["support.other.variable", "string.other.link"],
                "settings": { "foreground": "#ff5e5eff" }
            },
            {
                "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
                "scope": [
                    "constant.numeric",
                    "constant.language",
                    "support.constant",
                    "constant.character",
                    "constant.escape",
                    "variable.parameter",
                    "keyword.other.unit",
                    "keyword.other"
                ],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "String, Symbols, Inherited Class, Markup Heading",
                "scope": [
                    "string",
                    "constant.other.symbol",
                    "constant.other.key",
                    "entity.other.inherited-class",
                    "markup.heading",
                    "markup.inserted.git_gutter",
                    "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
                ],
                "settings": { "foreground": "#a07f00ff" }
            },
            {
                "name": "Class, Support",
                "scope": [
                    "entity.name",
                    "support.type",
                    "support.class",
                    "support.other.namespace.use.php",
                    "meta.use.php",
                    "support.other.namespace.php",
                    "markup.changed.git_gutter",
                    "support.type.sys-types"
                ],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "Entity Types",
                "scope": ["support.type"],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "CSS Class and Support",
                "scope": [
                    "source.css support.type.property-name",
                    "source.sass support.type.property-name",
                    "source.scss support.type.property-name",
                    "source.less support.type.property-name",
                    "source.stylus support.type.property-name",
                    "source.postcss support.type.property-name"
                ],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "Sub-methods",
                "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
                "settings": { "foreground": "#ff5e5eff" }
            },
            {
                "name": "Language methods",
                "scope": ["variable.language"],
                "settings": { "fontStyle": "italic", "foreground": "#ff5e5eff" }
            },
            {
                "name": "entity.name.method.js",
                "scope": ["entity.name.method.js"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "meta.method.js",
                "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Attributes",
                "scope": ["entity.other.attribute-name"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "HTML Attributes",
                "scope": ["text.html.basic entity.other.attribute-name.html", "text.html.basic entity.other.attribute-name"],
                "settings": { "fontStyle": "italic", "foreground": "#444444ff" }
            },
            {
                "name": "CSS Classes",
                "scope": ["entity.other.attribute-name.class"],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "CSS ID's",
                "scope": ["source.sass keyword.control"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Inserted",
                "scope": ["markup.inserted"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "Deleted",
                "scope": ["markup.deleted"],
                "settings": { "foreground": "#ff5e5eff" }
            },
            {
                "name": "Changed",
                "scope": ["markup.changed"],
                "settings": { "foreground": "#a07f00ff" }
            },
            {
                "name": "Regular Expressions",
                "scope": ["string.regexp"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Escape Characters",
                "scope": ["constant.character.escape"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "URL",
                "scope": ["*url*", "*link*", "*uri*"],
                "settings": { "fontStyle": "underline" }
            },
            {
                "name": "Decorators",
                "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
                "settings": { "fontStyle": "italic", "foreground": "#688790ff" }
            },
            {
                "name": "JSON Key - Level 0",
                "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "JSON Key - Level 1",
                "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "JSON Key - Level 2",
                "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "Markdown - Plain",
                "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Markdown - Heading",
                "scope": ["markdown.heading", "markup.heading | markup.heading entity.name", "markup.heading.markdown punctuation.definition.heading.markdown"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Markdown - Blockquote",
                "scope": ["markup.quote", "punctuation.definition.blockquote.markdown"],
                "settings": { "fontStyle": "italic", "foreground": "#444444ff" }
            },
            {
                "name": "Markdown - Link",
                "scope": ["string.other.link.title.markdown"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "Markdown - Link Description",
                "scope": ["string.other.link.description.title.markdown"],
                "settings": { "foreground": "#a07f00ff" }
            },
            {
                "name": "Markdown - Link Anchor",
                "scope": ["constant.other.reference.link.markdown"],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "Markup - Raw Block",
                "scope": ["markup.raw.block"],
                "settings": { "foreground": "#a07f00ff" }
            },
            {
                "name": "Markdown - Raw Block Fenced",
                "scope": ["markup.raw.block.fenced.markdown"],
                "settings": { "foreground": "#4444447f" }
            },
            {
                "name": "Markdown - Fenced Bode Block",
                "scope": ["punctuation.definition.fenced.markdown"],
                "settings": { "foreground": "#4444447f" }
            },
            {
                "name": "Markdown - Fenced Bode Block Variable",
                "scope": ["markup.raw.block.fenced.markdown", "variable.language.fenced.markdown", "punctuation.section.class.end"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Markdown - Fenced Language",
                "scope": ["variable.language.fenced.markdown"],
                "settings": { "foreground": "#444444ff" }
            },
            {
                "name": "Markdown - Separator",
                "scope": ["meta.separator"],
                "settings": { "fontStyle": "bold", "foreground": "#444444ff" }
            },
            {
                "name": "Markup - Table",
                "scope": ["markup.table"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Bracket Pair Colorization",
                "scope": ["brackethighlighter.default"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Shell - Shebang",
                "scope": ["punctuation.definition.comment.shebang.shell"],
                "settings": { "fontStyle": "bold", "foreground": "#444444ff" }
            },
            {
                "name": "Shell - Variables",
                "scope": ["variable.other.shell"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "Shell - Keywords",
                "scope": ["keyword.control.shell"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "Shell - Functions",
                "scope": ["entity.name.function.shell"],
                "settings": { "foreground": "#688790ff" }
            },
            {
                "name": "Shell - Strings",
                "scope": ["string.quoted.double.shell", "string.quoted.single.shell"],
                "settings": { "foreground": "#a07f00ff" }
            },
            {
                "name": "Shell - Interpolation",
                "scope": ["string.interpolated.dollar.shell", "punctuation.definition.variable.shell"],
                "settings": { "foreground": "#ff5e5eff" }
            },
            {
                "name": "JS/TS - Import/Export keywords",
                "scope": ["keyword.control.module.js", "keyword.control.module.ts"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "JS/TS - Language keywords",
                "scope": ["keyword.control.flow.js", "keyword.control.flow.ts", "keyword.control.conditional.js", "keyword.control.conditional.ts", "keyword.control.loop.js", "keyword.control.loop.ts", "keyword.operator.new.js", "keyword.operator.new.ts", "keyword.control.trycatch.js", "keyword.control.trycatch.ts"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "JS/TS - Storage and Modifiers",
                "scope": ["storage.type.class.js", "storage.type.class.ts", "storage.modifier.async.js", "storage.modifier.async.ts"],
                "settings": { "foreground": "#2e7d32ff" }
            },
            {
                "name": "JS/TS - Language constants",
                "scope": ["constant.language.js", "constant.language.ts"],
                "settings": { "foreground": "#111111ff" }
            },
            {
                "name": "JS/TS - `this` keyword",
                "scope": ["variable.language.this.js", "variable.language.this.ts"],
                "settings": { "fontStyle": "italic", "foreground": "#ff5e5eff" }
            }
        ],
        "[*Light*]": {
            "textMateRules": [
                { "scope": "ref.matchtext", "settings": { "foreground": "#000" } }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                { "scope": "ref.matchtext", "settings": { "foreground": "#fff" } }
            ]
        }
    },

    "github.copilot.nextEditSuggestions.enabled": true,
    "python.analysis.typeCheckingMode": "basic",
    "files.autoSave": "onFocusChange",
    "keyboard.dispatch": "keyCode",
    "python.defaultInterpreterPath": "/home/shui/anki_test_audio/.venv",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}